<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.shoesbox.entity">
    <class name="com.shoesbox.entity.Brand" table="shoes_brand">
        <id name="brandId" column="BRAND_ID" type="java.lang.Integer">
            <generator class="identity"></generator>
        </id>
        <property name="brandDescription">
            <column name="BRAND_DESC" sql-type="varchar" />
        </property>
    </class>
</hibernate-mapping>